<dsx>
	<scene root="scene" front_face="CCW" depth_func="LEQUAL,enable" cull_face="back,enable" lighting="enable" shading="Gouraud" polygon_mode="fill" />
	 <views default="defaultview" >
	 	<perspective id="perspective1" near= "0.1" far="1000" angle="60">
	 		<from x="0" y="0" z="0" />
	 		<to x="2" y="2" z="2" />
	 	</perspective>
	 </views>

    <illumination doublesided="false" local="false" >
    
        <ambient r="0" g="0" b="0" a="1" />
        <background r="0" g="1" b="1" a="1" />
        
    </illumination>
    <lights>
    	<omni id="light0" enabled="true" >
            <location x="3" y="3" z="3" w="0" />
            <ambient r="0.3" g="0.3" b="0.3" a="0.3" />
            <diffuse r="0" g="0" b="0" a="1" />
            <specular r="0.7" g="0.7" b="0.5" a="1" />
        </omni>
    </lights>
</dsx>